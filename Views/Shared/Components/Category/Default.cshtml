@{
}

@model IEnumerable<string>

<div class="d-grid gap-2">
    <a asp-controller="Home" asp-action="Index" asp-route-categiryType="" asp-route-pageNum="" class="btn btn-outline-secondary ">Home</a>

    
    @foreach (string c in Model)
    {
        <a asp-controller="Home" asp-action="Index" asp-route-categoryType="@c" asp-route-pageNum="1"
           class="btn @(ViewBag.SelectedCategory == c ? "btn-secondary" : "btn-outline-secondary")">@c</a>
           
    }

    
</div>